#!groovy

pipeline {
    agent any
    stages {
            stage('step 1') {
               steps {
                    sh 'pwd'
                     }
            }
            stage('step 2') {
            steps {
                    sh 'ls -l'
                  }

            }
            stage('step 3') {
            steps {
                    sh 'cd ~'
                  }

            }
            stage('step 4') {
            steps {
                    sh 'ls -l'
                  }

            }
    }
}
